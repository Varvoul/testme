{
  "name": "quravel",
  "version": "1.0.0",
  "description": "Streaming platform for movies, TV shows, and anime - Built with 11ty",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npx @11ty/eleventy --serve",
    "build": "npx @11ty/eleventy",
    "dev": "npx @11ty/eleventy --serve --watch",
    "clean": "rm -rf _site",
    "build:prod": "npm run clean && NODE_ENV=production npx @11ty/eleventy",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "vercel-build": "npm run build:prod",
    "vercel-dev": "npm run dev",
    "netlify-build": "npm run build:prod",
    "netlify-dev": "npm run dev",
    "preview": "npm run build:prod && npx serve _site",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "streaming",
    "movies",
    "anime",
    "tv-shows",
    "11ty",
    "static-site",
    "quravel"
  ],
  "author": "Quravel",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/yourusername/quravel.git"
  },
  "homepage": "https://quravel.vercel.app",
  "funding": {
    "type": "individual",
    "url": "https://quravel.com/donate"
  },
  "cacheDirectories": [
    "node_modules",
    ".cache"
  ],
  "volta": {
    "node": "18.20.0"
  }
}